{
  "name": "@ionic-bot/solidjs-circular-progressbar",
  "version": "1.0.4",
  "description": "A circular progress indicator component",
  "author": "Kevin Qi <iqnivek@gmail.com>",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-bot/solidjs-circular-progressbar.git"
  },
  "license": "MIT",
  "keywords": [
    "progressbar",
    "solidjs",
    "solidjs-component",
    "svg"
  ],
  "scripts": {
    "build": "npm-run-all clean build:js build:css",
    "build:css": "postcss src/styles.css --use autoprefixer -d dist/ --no-map",
    "build:js": "rollup -c",
    "prepare": "npm-run-all clean build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "postcss-cli": "^11.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-preset-solid": "^2.0.1",
    "solid-js": "^1.8.20",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "solid-js": "^1.8.20"
  }
}
